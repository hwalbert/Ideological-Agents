<experiments>
  <experiment name="Candidate Strategy Experiments" repetitions="100" runMetricsEveryStep="true">
    <setup>setup</setup>
    <go>go</go>
    <metric>count turtles</metric>
    <metric>((count Voters with [voting? = TRUE]) / count Voters) * 100</metric>
    <metric>Winner</metric>
    <metric>Flip?</metric>
    <enumeratedValueSet variable="NetworkType">
      <value value="&quot;ScaleFreeNetwork&quot;"/>
      <value value="&quot;RandomNetwork&quot;"/>
      <value value="&quot;SmallWorldNetwork&quot;"/>
    </enumeratedValueSet>
