;Ideological Agents
;Harold Walbert - haroldwalbert@gmail.com
extensions [nw]

breed [Candidates Candidate]
breed [Voters Voter]
globals [
  CandidateR
  CandidateL
  Flip?
  Winner
  ]
turtles-own [
  EF ; Economic Freedom: x-axis
  PF ; Personal Freedom: y-axis
  DT ; a third dimension of political ideology

  myCandidate
  notMyCandidate

]
Candidates-own [VotersICareAbout]

Voters-own [
  P ; "the subjective probability that one's vote will bring about a difference in policy outcomes" - Stevens P.182
  B ; "net utility gain from a difference in policy outcomes (this will be the absolute value of the distance between Voter and Candidate
  D ; "private benefits from the act of voting" - we are treating as negligable and random and normally distributed
  C ; "the costs of voting"
  F ; "the chance that the Voters preferred candidate will win"

  Influencers
  InfluencersPro
  Fervency
  ;myCandSupport
  ;myCandNoSupport
  PBD
  voting?
  ]

to go
  ifelse count voters with [color = red and voting? = TRUE] > count voters with [color = blue and voting? = TRUE] [set Winner "CandidateR"] [set Winner "CandidateL"]
  set Flip? count voters with [myCandidate = CandidateR] < count voters with [myCandidate = CandidateL] and count voters with [color = red and voting? = TRUE] < count voters with [color = blue and voting? = TRUE]
  ;ifelse count voters with [color = red] < count voters with [color = blue] AND
  if ticks = 20 [
    stop
    ]

  pollVoters
  updateVoterVariables
  tick

end

to setup
  setupGraph
  setupVotersAndCandidates
  setVoterVariables
  set Flip? "NA"
end

to setVoterVariables
  CalcB
  CalcF
  CalcP
  CalcC
  CalcD
  DecideToVote
end

to updateVoterVariables
  CalcB
  CalcF
  CalcP
  DecideToVote
end


to CalcB
  ask Voters [
    ;find the 3D ideological distance between the agent and each candidate
    let dr (distancexyz-nowrap ([xcor] of CandidateR) ([ycor] of CandidateR) ([zcor] of CandidateR))
    let dl (distancexyz-nowrap ([xcor] of CandidateL) ([ycor] of CandidateL) ([zcor] of CandidateL))

    ;set the agents candidate based on the ideological distance
    ifelse dr > dl
    [set myCandidate CandidateL
     set notMyCandidate CandidateR]
    [set myCandidate CandidateR
     set notMyCandidate CandidateL]

    ;Finally calculate B which is the the distance from the person thats not your candidate minus the distance to the person that is your candidate
    if myCandidate = CandidateR [
     set color Red
     set B dl - dr
     ]
    if myCandidate = CandidateL [
     set color Blue
     set B dr - dl
     ]
  ]
end


to CalcF
  ask Voters [
    set Influencers other Voters with [link-neighbor? myself]
    set InfluencersPro other Influencers with [myCandidate = [myCandidate] of myself]

    ifelse any? Influencers and any? InfluencersPro and mean [B] of Influencers != 0 [

      set F (((count InfluencersPro) * mean [B] of InfluencersPro) / ((count Influencers) * mean [B] of Influencers))
      ;set F mean [B] of Influencers

      ] [set F random-normal 0.5 0.05]
  ]
end


to CalcP
  ask Voters [set P (3 * e ^ (-2 * (count Voters - 1) * (F - .5) ^ 2) ) / (2 * sqrt((2 * pi) * (count Voters - 1)))]
end



to CalcC
  if VotingCost = "NoCost" [ask Voters [set C 0]]
  if VotingCost = "SetCost" [ask Voters [
      set C random-normal Cost .01
      if C < 0 [set C 0]
      ]
  ]
end

to CalcD
  ifelse Corruption? = TRUE [
    ask Voters [
      set D abs(random-exponential 1)
      if D < 1 [set D 1]
      ]
    ][
    ask Voters [
      set D 1
      ]
    ]
end

to DecideToVote
  ask voters [
    set PBD (P * B * D)
    ifelse PBD > C [set voting? TRUE][set voting? FALSE]
    ]
end





;this is the first thing we do:
to setupGraph
  ca
  reset-ticks
;the model starts with the import of a network generated according to a random, scale free or small world distribution
  if NetworkType = "RandomNetwork" [nw:load-graphml "C:/Users/Owner/Documents/GitHub/Ideological-Agents/Data/Random_GraphForNetLogoModel.graphml" [
    ask links [hide-link]
    ]]
  if NetworkType = "ScaleFreeNetwork" [nw:load-graphml "C:/Users/Owner/Documents/GitHub/Ideological-Agents/Data/ScaleFree_GraphForNetLogoModel.graphml" [
    ask links [hide-link]
    ]]
  if NetworkType = "SmallWorldNetwork" [nw:load-graphml "C:/Users/Owner/Documents/GitHub/Ideological-Agents/Data/SmallWorld_GraphForNetLogoModel.graphml" [
    ask links [hide-link]
    ]]
end

;then setup voters and candidates
to setupVotersAndCandidates
  reset-ticks
  setupVoters
  setupCandidates
end

;this sets up the voters with a distribution based on the empirically observed voter ideal points
to setupVoters
  ask turtles[
    ;set their xyz coordinates to a random normal with the mean at the empirical ideal point
    setxyz (random-normal EF 1) (random-normal PF 1) (random-normal DT 1)
    set color green
    set breed Voters
    ]

  ask Voters[
    ask Voters [set size .25]
    ask Voters [set shape "circle"]
    ]
end

to setupCandidates
  create-Candidates 2 [
  ]

  set CandidateR turtle max [who] of candidates
  set CandidateL turtle min [who] of candidates


  ifelse RandomInitialCandidatePositions? = TRUE [
    ask Candidates[
    setxyz random 9 random 9 random 9
    if random 2 = 0 [set xcor (xcor * -1)]
    if random 2 = 0 [set ycor (ycor * -1)]
    if random 2 = 0 [set zcor (zcor * -1)]
    set shape "circle"
    set color White
    set size 1
    set pen-mode "down"
    ]
    ][
  ask CandidateR [set EF EF_CandidateR]
  ask CandidateR [set PF PF_CandidateR]
  ask CandidateR [set DT DT_CandidateR]
  ask CandidateL [set EF EF_CandidateL]
  ask CandidateL [set PF PF_CandidateL]
  ask CandidateL [set DT DT_CandidateL]
  ask Candidates [
    setxyz EF PF DT
    set shape "circle"
    set color White
    set size 1
    set pen-mode "down"
    ]
  ]


;  ask candidates [
;    setxyz EF PF DT
;    set shape "circle"
;    set color White
;    set size 1
;    set pen-mode "down"
;  ]

end




to moveCandidatePositions
  ask CandidateR [set shape "arrow"]
  ask CandidateR [facexyz mean [EF] of voters mean [PF] of voters mean [DT] of voters]

  ask CandidateL [set shape "arrow"]
  ask CandidateL [facexyz mean [EF] of voters mean [PF] of voters mean [DT] of voters]
end



to pollVoters
  ask CandidateR [
    set shape "arrow"
    let myPollVoters n-of 30 Voters
    let myVoters myPollVoters with [myCandidate = myself]
    let notMyVoters myPollVoters with [myCandidate != myself]
    ;this throws out some of the people in the poll that don't support the current candidate
    let discountNotMyVoters n-of (discountR * count NotMyVoters) notMyVoters
    ;this set an agentset of voters that I will consider and try to move towards
    set VotersICareAbout (turtle-set discountNotMyVoters myVoters)
    ;this makes the candidate face the direction of the voters they care about
    facexyz mean [EF] of VotersICareAbout mean [PF] of VotersICareAbout mean [DT] of VotersICareAbout
    ;this moves the candidate in the direction of the voters they care about
    forward 1
    ]

    ask CandidateL [
    set shape "arrow"
    let myPollVoters n-of 30 Voters
    let myVoters myPollVoters with [myCandidate = myself]
    let notMyVoters myPollVoters with [myCandidate != myself]
    ;this throws out some of the people in the poll that don't support the current candidate
    let discountNotMyVoters n-of (discountL * count NotMyVoters) notMyVoters
    ;this set an agentset of voters that I will consider and try to move towards
    set VotersICareAbout (turtle-set discountNotMyVoters myVoters)
    ;this makes the candidate face the direction of the voters they care about
    facexyz mean [EF] of VotersICareAbout mean [PF] of VotersICareAbout mean [DT] of VotersICareAbout
    ;this moves the candidate in the direction of the voters they care about
    forward 1
    ]
end


